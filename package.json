  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "release": "semantic-release --no-ci"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
